# AI Tester Environment Configuration Template
# Copy this file to .env and fill in your values

# Google Gemini API Key (required)
# Get your API key from: https://ai.google.dev/
GOOGLE_API_KEY=your_gemini_api_key_here

# Gemini Model (optional - uses default if not specified)
GEMINI_MODEL=gemini-2.5-computer-use-preview-10-2025

# Debug mode (optional - enables verbose logging)
DEBUG=false

# Page load timeout in milliseconds (optional)
TIMEOUT_MS=30000